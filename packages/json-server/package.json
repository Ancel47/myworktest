{
  "version": "0.1.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch --tsconfig tsconfig.json --verbose --noClean",
    "build": "tsdx build --tsconfig tsconfig.json",
    "test": "tsdx test --passWithNoTests --runInBand",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "name": "refinejs-json-server",
  "author": "Pankod",
  "module": "dist/refinejs-json-server.esm.js",
  "size-limit": [
    {
      "path": "dist/refinejs-json-server.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/refinejs-json-server.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.10.1",
    "husky": "^5.2.0",
    "nock": "^13.0.11",
    "size-limit": "^4.10.1",
    "tsdx": "^0.14.1",
    "tslib": "^2.1.0"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "query-string": "^7.0.0",
    "refinejs": "^1.0.0"
  }
}

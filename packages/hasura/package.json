    {
      "path": "dist/refine-hasura.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^5.0.5",
    "nock": "^13.1.3",
    "size-limit": "^5.0.5",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "@pankod/refine": "^2.0.7",
    "gql-query-builder": "^3.5.5",
    "graphql": "^15.6.1",
    "graphql-request": "^3.5.0"
